{
  "storage_type": "minio",
  "storage": {
    "endPoint": "minio",
    "region": "local",
    "pathStyle": true,
    "bucket": "swanlab-public",
    "port": 9000,
    "useSSL": false
  },
  "private-storage": {
    "endPoint": "minio",
    "region": "local",
    "pathStyle": true,
    "bucket": "swanlab-private",
    "port": 9000,
    "useSSL": false
  },
  "session": {
    "secure": false
  },
  "avatar": {
    "prefix": "avatars",
    "domain": ""
  },
  "sts": {
    "durationSeconds": 7200,
    "prefix": "resources/",
    "endPoint": "http://minio:9000",
    "region": "local",
    "bucket": "swanlab-private"
  },
  "house_url": "http://swanlab-house:3000/api/house",
  "jwt": {
    "publicPath": "/app/config/public.pem"
  },
  "self-host": {
    "enabled": true
  },
  "cloud_url": "https://dev001.swanlab.cn/api"
}
